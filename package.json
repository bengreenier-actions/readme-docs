{
  "name": "readme-docs",
  "version": "0.1.0",
  "private": true,
  "description": "Action to upload docs to readme.com",
  "main": "lib/main.js",
  "type": "commonjs",
  "bin": {
    "readme-docs": "bin/readme-docs.js"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build -C --source-map --license licenses.txt",
    "test": "jest --coverage",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bengreenier-actions/readme-reference.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Ben Greenier",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/glob": "^0.2.0",
    "cockatiel": "^3.0.0-beta.0",
    "global-agent": "^3.0.0",
    "gray-matter": "^4.0.3",
    "node-fetch": "^3.2.0",
    "node-fetch-cjs": "^4.0.0-beta.4"
  },
  "devDependencies": {
    "@types/global-agent": "^2.1.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.10.5",
    "@typescript-eslint/parser": "^5.18.0",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^25.3.2",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "memfs": "^3.4.1",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": ">=16"
  }
}
